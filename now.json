{
	"version": 2,
	"name": "wikium-test",
	"alias": "wikium-test.now.sh",
	"builds": [
		{
			"src": "package.json",
			"use": "@now/static-build"
		}
	],
	"routes": [
		{
			"src": "^/css/(.*)",
			"dest": "/css/$1"
		},
		{
			"src": "^/img/(.*)",
			"dest": "/img/$1"
		},
		{
			"src": "^/js/(.*)",
			"dest": "/js/$1"
		},
		{
			"src": "^/upload/(.*)",
			"dest": "/upload/$1"
		},
		{
			"src": "^/favicon/(.*)",
			"dest": "/favicon/$1"
		},
		{
			"src": "^/favicon(\\.\\w+)",
			"dest": "/favicon$1"
		},
		{
			"src": "(.*)",
			"dest": "$1"
		}
	]
}
